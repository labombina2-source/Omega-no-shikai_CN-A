@freeimage layer=4
[endmacro]

[macro name=sub_title]
@cm
@stopbgm
@img_cha_r_free
@img_cha_c_free
@img_cha_l_free
@layopt layer=3 visible=false
@layopt layer=4 visible=false
@layopt layer=5 visible=false
@layopt layer=6 visible=false
@layopt layer=7 visible=false

@image storage="img_bg_b" layer=base page=back
@position layer=message1 visible=false
@position layer=message2 visible=false
@position layer=message0 visible=false
@backlay
@image storage=%img layer=base page=back
@trans method=crossfade time=1500
@wt
@waitclick
@position layer=message0 frame="img_m_f_01A" visible=true
@current  layer=message0
[endmacro]

[macro name=mes_normal]
@cm
@position layer=message1 visible=false
@position layer=message2 visible=false
@position layer=message0 visible=true
@position layer=message0 frame="img_m_f_01A" visible=true
@current  layer=message0
[endmacro]

[macro name=mes_all]
@cm
@position layer=message0 visible=false
@position layer=message1 visible=true
@position layer=message2 visible=false
@position layer=message1 visible=true page=fore top=0 left=0 width=800 height=600 frame=""
@current  layer=message1 
[endmacro]

[macro name=mes_all_full]
