;//マクロ-----------------------------------
[macro name=pc]
[p][cm]
[endmacro]
[macro name=lr]
[l][r]
[endmacro]

[macro name=img_bg]
[image storage=%img page=fore layer=base]
[endmacro]

[macro name=img_cha_r]
[image storage=%img page=fore visible=true layer=1 left=400]
[endmacro]

[macro name=img_cha_c]
[image storage=%img page=fore visible=true layer=2 left=150 top=%top]
[endmacro]

[macro name=img_cha_l]
[image storage=%img page=fore visible=true layer=3 left=-80]
[endmacro]

[macro name=img_bg_trans]
[backlay]
[image storage=%img layer=base page=back]
[trans method=crossfade time=%time|1500]
[wt]
[endmacro]

[macro name=img_bg_rule]
[backlay]
[image storage=%img layer=base page=back]
[trans method=universal rule=%rule time=%time|1500]
[wt]
[endmacro]


[macro name=img_cha_r_free]
@freeimage layer=1
[endmacro]
[macro name=img_cha_c_free]
@freeimage layer=2
[endmacro]
[macro name=img_cha_l_free]
@freeimage layer=3
[endmacro]

[macro name=f_cha]
@layopt layer=5 autohide=true
@layopt layer=4 autohide=true
@image storage=%img page=fore visible=true layer=5 top=160 left=15 autohide=true
@image storage="img_bg_b" page=fore visible=true layer=4 opacity=100 autohide=true
[endmacro]

[macro name=f_cha_free]
